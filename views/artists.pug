include header.pug
h2 Artists

.flexbox(class="z-depth-5")
   for item in artists
      .flexcard
         p(class='id')=item.id
         h4=item.name
         p= 'gender:'+ item.gender
         if(item.yearOfDeath==null)
            p= 'born: '+ item.yearOfBirth
         else
            p= 'born/died: '+ item.yearOfBirth +' - ' +item.yearOfDeath


include footer.pug